<% provide(:title, "Log In") %>

<section>
<h1>Log In</h1>
<section>

<section>
<%= form_for(:session, url: login_path) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Log In" %>
<% end %>
<section>

<section>
<p>New user? <%= link_to "Sign up now!", signup_path %></p>
<section>
